# Copy to .env before running locally
# cp .env.example .env

# RPC URL for local or remote Ethereum node
RPC_URL=http://localhost:8545

# Network name (e.g., Ethereum mainnet, Rinkeby, Sepolia, or custom)
NETWORK=development

# Optional API key for any external services (if your environment requires one)
API_KEY=

# Logging level for debugging (debug | info | warn)
LOG_LEVEL=info
